@model IEnumerable<LindaniDrivingSchool.Models.Course>

@{
    ViewBag.Title = "LearnerCourses";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var course = ViewBag.courses;
}
<style>
    .md-v-line {
        position: absolute;
        border-left: 1px solid rgba(0,0,0,.125);
        height: 50px;
        top: 0px;
        left: 54px;
    }
</style>

@Html.Partial("_LearnerPortal")
<ul class="list-group">
    @foreach (var item in course)
    {   
        <li class="list-group-item ">
            <div class="md-v-line"></div><i class="fas fa-laptop mr-4 pr-3"></i> @item.Name
        </li>
    }
</ul>

    </div>
    </div>
